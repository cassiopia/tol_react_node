{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "watch-ts": "tsc -w",
    "build": "tsc"
  },
  "dependencies": {
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "express": "^4.16.4",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "debug": "~2.6.9",
    "@types/express": "^4.17.3",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
